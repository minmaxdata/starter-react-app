    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "html-webpack-loader": "0.0.5",
    "html-webpack-plugin": "^3.2.0",
    "json-server": "^0.16.1",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
